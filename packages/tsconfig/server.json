{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./base.json",
  "compilerOptions": {
    "noEmit": false,
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
